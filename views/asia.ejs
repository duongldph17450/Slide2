<!doctype html>
<html lang="en">
<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <title>Ocean Wallpaper</title>
</head>
<body class="container">
<% include nav %>

<h1> <%=title%> </h1>

<h1> Name: <%=name%> </h1>

<h1> Age: <%=age%> </h1>

<h1>
  <% for (var i = 0; i < array.length; i++) { %>

    <div class="btn btn-outline-primary"><%=array[i]%></div>

  <% } %>
</h1>

<h2><%=students.name%></h2>
<h2><%=students.phoneNumber%></h2>

<% for (var i = 0; i < lists.length; i++) { %>

  <div class="btn btn-outline-primary"><%=lists[i].albumId%></div>
  <div class="btn btn-primary"><%=lists[i].id%></div>
  <div>--------------------------------------------------</div>
<% } %>

<!-- Gallery -->
<div class="row">
  <div class="col-lg-4 col-md-12 mb-4 mb-lg-0">
    <img
            src="https://mdbcdn.b-cdn.net/img/Photos/Horizontal/Nature/4-col/img%20(73).webp"
            class="w-100 shadow-1-strong rounded mb-4"
            alt="Boat on Calm Water"
    />

    <img
            src="https://thumbs.dreamstime.com/b/admirable-atlantic-ocean-coastline-high-cliffs-etretat-normandy-france-picturesque-landscape-wonderful-beaches-europe-156360860.jpg"
            class="w-100 shadow-1-strong rounded mb-4"
            alt="random ocean"
    />

    <img
            src="https://youimg1.tripcdn.com/target/0103n120008ehzeoy1C82.jpg?proc=source%2Ftrip"
            class="w-100 shadow-1-strong rounded mb-4"
            alt="random ocean"
    />
  </div>

  <div class="col-lg-4 mb-4 mb-lg-0">
    <img
            src="https://images.squarespace-cdn.com/content/v1/5fa57aacf5b0a90a76b0d7cc/1633353350132-OSHNT7ORGVTNJLX6BX27/becoming-carmen-travel-france-normandy-etretat"
            class="w-100 shadow-1-strong rounded mb-4"
            alt="Mountains in the Clouds"
    />

    <img
            src="https://mdbcdn.b-cdn.net/img/Photos/Horizontal/Nature/4-col/img%20(73).webp"
            class="w-100 shadow-1-strong rounded mb-4"
            alt="Boat on Calm Water"
    />

    <img
            src="https://c4.wallpaperflare.com/wallpaper/164/316/890/landscape-france-view-etretat-wallpaper-thumb.jpg"
            class="w-100 shadow-1-strong rounded mb-4"
            alt="Boat on Calm Water"
    />
  </div>

  <div class="col-lg-4 mb-4 mb-lg-0">
    <img
            src="https://mdbcdn.b-cdn.net/img/Photos/Horizontal/Nature/4-col/img%20(18).webp"
            class="w-100 shadow-1-strong rounded mb-4"
            alt="Waves at Sea"
    />

    <img
            src="https://previews.agefotostock.com/previewimage/medibigoff/a09dc73015bf92056b41148fbcb6930c/xd8-760882.jpg"
            class="w-100 shadow-1-strong rounded mb-4"
            alt="Yosemite National Park"
    />

    <img
            src="https://v7k3m3s6.rocketcdn.me/wp-content/uploads/2019/frankreich/etretat-felsformation-01.jpg"
            class="w-100 shadow-1-strong rounded mb-4"
            alt="Yosemite National Park"
    />
  </div>
</div>
<!-- Gallery -->


<!-- Option 1: Bootstrap Bundle with Popper -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>

